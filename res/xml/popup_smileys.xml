<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
    limehd:keyWidth="15%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >

    <Row limehd:rowEdgeFlags="top">
        <Key limehd:keyLabel=":-)" limehd:keyOutputText=":-) " limehd:keyEdgeFlags="left" />
        <Key limehd:keyLabel=":-(" limehd:keyOutputText=":-( "/>
        <Key limehd:keyLabel=";-)" limehd:keyOutputText=";-) "/>
        <Key limehd:keyLabel=":-P" limehd:keyOutputText=":-P "/>
        <Key limehd:keyLabel="=-O" limehd:keyOutputText="=-O " limehd:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key limehd:keyLabel=":-*" limehd:keyOutputText=":-* " limehd:keyEdgeFlags="left" />
        <Key limehd:keyLabel=":O" limehd:keyOutputText=":O "/>
        <Key limehd:keyLabel="B-)" limehd:keyOutputText="B-) "/>
        <Key limehd:keyLabel=":-$" limehd:keyOutputText=":-$ "/>
        <Key limehd:keyLabel=":-!" limehd:keyOutputText=":-! " limehd:keyEdgeFlags="right" />
    </Row>
    <Row limehd:rowEdgeFlags="bottom">
        <Key limehd:keyLabel=":-[" limehd:keyOutputText=":-[ " limehd:keyEdgeFlags="left" />
        <Key limehd:keyLabel="O:-)" limehd:keyOutputText="O:-) "/>
        <Key limehd:keyLabel=":-\\" limehd:keyOutputText=":-\\ "/>
        <Key limehd:keyLabel=":'(" limehd:keyOutputText=":'( "/>
        <Key limehd:keyLabel=":-D" limehd:keyOutputText=":-D " limehd:keyEdgeFlags="right" />
    </Row>
</Keyboard>
