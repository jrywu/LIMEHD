<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
    limehd:keyWidth="15%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >

    <Row limehd:rowEdgeFlags="top|bottom">
        <Key limehd:keyLabel="@string/popular_domain_1"
            limehd:keyOutputText="@string/popular_domain_1" limehd:keyEdgeFlags="left" />
        <Key limehd:keyLabel="@string/popular_domain_2"
            limehd:keyOutputText="@string/popular_domain_2"/>
        <Key limehd:keyLabel="@string/popular_domain_3"
            limehd:keyOutputText="@string/popular_domain_3"/>
        <Key limehd:keyLabel="@string/popular_domain_4"
            limehd:keyOutputText="@string/popular_domain_4"/>
        <Key limehd:keyLabel="@string/popular_domain_5"
            limehd:keyOutputText="@string/popular_domain_5" limehd:keyEdgeFlags="right" />
    </Row>
</Keyboard>
