				limehd:keyWidth="10%p" />
		<Key limehd:codes="47" limehd:keyLabel="/\nㄥ" limehd:keyWidth="10%p" />
        <Key limehd:codes="-2" limehd:keyLabel="123" limehd:isModifier="true"/>
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return" limehd:iconPreview="@drawable/sym_keyboard_feedback_return" limehd:isModifier="true" limehd:keyWidth="10%p" limehd:keyEdgeFlags="right"/>
