        <Key
            limehd:codes="50"
            limehd:keyLabel="2\t母\n車"
            limehd:popupCharacters="=\n母"
            limehd:popupKeyboard="@xml/popup_template" />
        <Key
            limehd:codes="51"
            limehd:keyLabel="3\t匚\n糸"
            limehd:popupCharacters="[\n匚"
            limehd:popupKeyboard="@xml/popup_template" />
        <Key
            limehd:codes="52"
            limehd:keyLabel="4\t]\n言"
            limehd:popupCharacters="]\n]"
            limehd:popupKeyboard="@xml/popup_template" />
