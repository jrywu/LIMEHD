<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
    limehd:keyWidth="10%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key limehd:codes="49" limehd:keyLabel="1" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="50" limehd:keyLabel="2"/>
        <Key limehd:codes="51" limehd:keyLabel="3"/>
        <Key limehd:codes="52" limehd:keyLabel="4"/>
        <Key limehd:codes="53" limehd:keyLabel="5"/>
        <Key limehd:codes="54" limehd:keyLabel="6"/>
        <Key limehd:codes="55" limehd:keyLabel="7"/>
        <Key limehd:codes="56" limehd:keyLabel="8"/>
        <Key limehd:codes="57" limehd:keyLabel="9"/>
        <Key limehd:codes="48" limehd:keyLabel="0" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key limehd:codes="64" limehd:keyLabel="\@" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="35" limehd:keyLabel="\#"/>
        <Key limehd:codes="36" limehd:keyLabel="$"/>
        <Key limehd:codes="37" limehd:keyLabel="%"/>
        <Key limehd:codes="38" limehd:keyLabel="&amp;"/>
        <Key limehd:codes="42" limehd:keyLabel="*"/>
        <Key limehd:codes="45" limehd:keyLabel="-"/>
        <Key limehd:codes="43" limehd:keyLabel="+"/>
        <Key limehd:codes="40" limehd:keyLabel="("/>
        <Key limehd:codes="41" limehd:keyLabel=")" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift"
                limehd:keyWidth="15%p" limehd:isModifier="true"
                limehd:isSticky="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="33" limehd:keyLabel="!" />
        <Key limehd:codes="34" limehd:keyLabel="&quot;"/>
        <Key limehd:codes="39" limehd:keyLabel="\'"/>
        <Key limehd:codes="58" limehd:keyLabel=":"/>
        <Key limehd:codes="59" limehd:keyLabel=";"/>
        <Key limehd:codes="47" limehd:keyLabel="/" />
        <Key limehd:codes="63" limehd:keyLabel="\?"/>
        <Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete" limehd:iconPreview="@drawable/sym_keyboard_feedback_delete" limehd:isModifier="true"
                limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"
                limehd:isRepeatable="true"/>
    </Row>
    
    <Row  limehd:rowEdgeFlags="bottom">
        <Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done" limehd:iconPreview="@drawable/sym_keyboard_feedback_done" limehd:isModifier="true"
                limehd:keyWidth="15%p" limehd:keyEdgeFlags="left" />
        <Key limehd:codes="-2" limehd:isModifier="true" limehd:keyLabel="EN"  limehd:keyWidth="10%p" />
        <Key limehd:codes="44" limehd:keyLabel="," limehd:keyWidth="10%p" />
        <Key limehd:codes="32" limehd:keyIcon="@drawable/sym_keyboard_space" 
        	 limehd:iconPreview="@drawable/sym_keyboard_feedback_space"
        	 limehd:keyWidth="25%p" limehd:isRepeatable="true"/>
        <Key limehd:codes="46" limehd:keyLabel="." 
        						 limehd:popupKeyboard="@xml/popup_punctuation" 
        						limehd:keyWidth="10%p"/>
        <Key limehd:codes="-10" limehd:isModifier="true" limehd:keyLabel="ä¸­"  limehd:keyWidth="10%p" />
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return" limehd:iconPreview="@drawable/sym_keyboard_feedback_return" limehd:isModifier="true"
                limehd:keyWidth="20%p" limehd:keyEdgeFlags="right"
                />
    </Row>
</Keyboard>