<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
    limehd:keyWidth="20%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/phone_key_height"
    >

    <Row>
        <Key limehd:codes="-2" limehd:isModifier="true" limehd:keyLabel="123" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="49" limehd:keyLabel="1"/>
        <Key limehd:codes="50" limehd:keyLabel="2"/>
        <Key limehd:codes="51" limehd:keyLabel="3"/>
        <Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete" limehd:iconPreview="@drawable/sym_keyboard_feedback_delete" limehd:isModifier="true"  limehd:keyEdgeFlags="right" limehd:isRepeatable="true"/>
    </Row>
    
    <Row>
        <Key limehd:codes="-9" limehd:keyLabel="ABC" limehd:isModifier="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="52" limehd:keyLabel="4"/>
        <Key limehd:codes="53" limehd:keyLabel="5"/>
        <Key limehd:codes="54" limehd:keyLabel="6"/>
        <Key limehd:codes="61,43,45,42,47" limehd:iconPreview="@drawable/phone_cal_l" limehd:keyIcon="@drawable/phone_cal" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift"
                limehd:isSticky="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="55" limehd:keyLabel="7"/>
        <Key limehd:codes="56" limehd:keyLabel="8"/>
        <Key limehd:codes="57" limehd:keyLabel="9"/>
        <Key limehd:codes="32" limehd:keyIcon="@drawable/sym_keyboard_space_lime" limehd:iconPreview="@drawable/sym_keyboard_feedback_space" limehd:isRepeatable="true" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row limehd:rowEdgeFlags="bottom">
        <Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done" limehd:iconPreview="@drawable/sym_keyboard_feedback_done" limehd:isModifier="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="44" limehd:keyLabel=","/>
        <Key limehd:codes="48" limehd:keyLabel="0"/>
        <Key limehd:codes="46" limehd:keyLabel="."  limehd:popupKeyboard="@xml/popup_c_punctuation" />
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return" limehd:iconPreview="@drawable/sym_keyboard_feedback_return" limehd:isModifier="true"  limehd:keyEdgeFlags="right"/>
    </Row>
    
</Keyboard>
    