		<Key limehd:codes="90" limehd:keyLabel="Z\nㄠ" />
		<Key limehd:codes="88" limehd:keyLabel="X\nㄨ" />
		<Key limehd:codes="67" limehd:keyLabel="C\nㄒ" />
