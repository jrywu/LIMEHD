			 limehd:keyEdgeFlags="right"	limehd:isRepeatable="true" />
	</Row>

		<Key limehd:codes="44" limehd:keyLabel=",\nㄓ"/>
		<Key limehd:codes="46" 
				limehd:keyLabel=".\nㄔ"
				 limehd:popupKeyboard="@xml/popup_c_punctuation"   
