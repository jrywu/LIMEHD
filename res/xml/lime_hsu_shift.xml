<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
    limehd:keyWidth="10%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >

  
	<Row>
		<Key limehd:codes="33" limehd:keyLabel="!"  limehd:keyEdgeFlags="left" />
		<Key limehd:codes="64" limehd:keyLabel="\@" />
		<Key limehd:codes="35" limehd:keyLabel="\#" />
		<Key limehd:codes="36" limehd:keyLabel="$" />
		<Key limehd:codes="37" limehd:keyLabel="%" />
		<Key limehd:codes="94" limehd:keyLabel="^" />
		<Key limehd:codes="38" limehd:keyLabel="&amp;" />
		<Key limehd:codes="42" limehd:keyLabel="*" />
		<Key limehd:codes="40" limehd:keyLabel="(" />
		<Key limehd:codes="41" limehd:keyLabel=")" limehd:keyEdgeFlags="right" />
	</Row>
    
    <Row>
        <Key limehd:codes="81" limehd:keyLabel="Q" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="87" limehd:keyLabel="W\nㄠ"/>
        <Key limehd:codes="69" limehd:keyLabel="E\tㄧ\nㄝ"/>
        <Key limehd:codes="82" limehd:keyLabel="R\tㄖ\nㄚ"/>
        <Key limehd:codes="84" limehd:keyLabel="T\nㄊ"/>
        <Key limehd:codes="89" limehd:keyLabel="Y\nㄚ"/>
        <Key limehd:codes="85" limehd:keyLabel="U\nㄩ"/>
        <Key limehd:codes="73" limehd:keyLabel="I\nㄞ"/>
        <Key limehd:codes="79" limehd:keyLabel="O\nㄡ"/>
        <Key limehd:codes="80" limehd:keyLabel="P\nㄆ" limehd:keyEdgeFlags="right"/>
    </Row>
    
    
    <Row>
        <Key limehd:codes="65" limehd:keyLabel="A\tㄘ\nㄟ" limehd:horizontalGap="5%p" 
                limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="83" limehd:keyLabel="S\t˙\nㄙ"/>
        <Key limehd:codes="68" limehd:keyLabel="D\tˊ\nㄉ"/>
        <Key limehd:codes="70" limehd:keyLabel="F\tˇ\nㄈ"/>
        <Key limehd:codes="71" limehd:keyLabel="G\tㄍ\nㄜ"/>
        <Key limehd:codes="72" limehd:keyLabel="H\tㄏ\nㄛ"/>
        <Key limehd:codes="74" limehd:keyLabel="J\tˋ\nㄐㄓ"/>
        <Key limehd:codes="75" limehd:keyLabel="K\tㄎ\nㄤ"/>
        <Key limehd:codes="76" limehd:keyLabel="L\tㄌ\nㄦㄥ" limehd:keyEdgeFlags="right"/>
    </Row>
 
    
    <Row>
        <Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift" limehd:iconPreview="@drawable/sym_keyboard_feedback_shift" 
                limehd:keyWidth="15%p" limehd:isModifier="true"
                limehd:isSticky="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="90" limehd:keyLabel="Z\nㄗ"/>
        <Key limehd:codes="88" limehd:keyLabel="X\nㄨ"/>
        <Key limehd:codes="67" limehd:keyLabel="C\tㄒ\nㄕ"/>
        <Key limehd:codes="86" limehd:keyLabel="V\tㄑ\nㄔ"/>
        <Key limehd:codes="66" limehd:keyLabel="B\nㄅ"/>
        <Key limehd:codes="78" limehd:keyLabel="N\tㄋ\nㄣ"/>
        <Key limehd:codes="77" limehd:keyLabel="M\tㄇ\nㄢ"/>
        <Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete" limehd:iconPreview="@drawable/sym_keyboard_feedback_delete" limehd:isModifier="true" 
                limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"
                limehd:isRepeatable="true"/>
    </Row>

    <Row limehd:rowEdgeFlags="bottom">
        <Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done" limehd:iconPreview="@drawable/sym_keyboard_feedback_done" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="left"/>                
        <Key limehd:codes="-9" limehd:keyLabel="EN" limehd:isModifier="true" limehd:keyWidth="10%p" />
        <Key limehd:codes="58" limehd:keyLabel=":" limehd:keyWidth="10%p"/>
        <Key limehd:codes="32" limehd:keyIcon="@drawable/sym_keyboard_space_lime" limehd:iconPreview="@drawable/sym_keyboard_feedback_space"  limehd:keyWidth="30%p" limehd:isRepeatable="true"/>
        <Key limehd:codes="59" 
        		limehd:keyLabel=";"
        		limehd:keyWidth="10%p"/>
        <Key limehd:codes="-2"  limehd:keyLabel="@string/label_symbol_key" limehd:isModifier="true"/>
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return" limehd:iconPreview="@drawable/sym_keyboard_feedback_return" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"/>
    </Row>
    
</Keyboard>
    