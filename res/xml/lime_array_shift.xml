<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
    limehd:keyWidth="10%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >
   
    <Row>
        <Key limehd:codes="81" limehd:keyLabel="Q\n1⇡" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="87" limehd:keyLabel="W\n2⇡"/>
        <Key limehd:codes="69" limehd:keyLabel="E\n3⇡"/>
        <Key limehd:codes="82" limehd:keyLabel="R\n4⇡"/>
        <Key limehd:codes="84" limehd:keyLabel="T\n5⇡"/>
        <Key limehd:codes="89" limehd:keyLabel="Y\n6⇡"/>
        <Key limehd:codes="85" limehd:keyLabel="U\n7⇡"/>
        <Key limehd:codes="73" limehd:keyLabel="I\n8⇡"/>
        <Key limehd:codes="79" limehd:keyLabel="O\n9⇡"/>
        <Key limehd:codes="80" limehd:keyLabel="P\n0⇡" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key limehd:codes="65" limehd:keyLabel="A\n1−" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="83" limehd:keyLabel="S\n2−"/>
        <Key limehd:codes="68" limehd:keyLabel="D\n3−"/>
        <Key limehd:codes="70" limehd:keyLabel="F\n4−"/>
        <Key limehd:codes="71" limehd:keyLabel="G\n5−"/>
        <Key limehd:codes="72" limehd:keyLabel="H\n6−"/>
        <Key limehd:codes="74" limehd:keyLabel="J\n7−"/>
        <Key limehd:codes="75" limehd:keyLabel="K\n8−"/>
        <Key limehd:codes="76" limehd:keyLabel="L\n9−"/>
        <Key limehd:codes="58" limehd:keyLabel=":\n0−" limehd:keyEdgeFlags="right"/>
    </Row> 
     <Row>
        <Key limehd:codes="90" limehd:keyLabel="Z\n1⇣" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="88" limehd:keyLabel="X\n2⇣"/>
        <Key limehd:codes="67" limehd:keyLabel="C\n3⇣"/>
        <Key limehd:codes="86" limehd:keyLabel="V\n4⇣"/>
        <Key limehd:codes="66" limehd:keyLabel="B\n5⇣"/>
        <Key limehd:codes="78" limehd:keyLabel="N\n6⇣"/>
        <Key limehd:codes="77" limehd:keyLabel="M\n7⇣"/>
        <Key limehd:codes="60" limehd:keyLabel="&lt;\n8⇣"/>
        <Key limehd:codes="62" limehd:keyLabel="&gt;\n9⇣"  limehd:popupKeyboard="@xml/popup_punctuation" />
        <Key limehd:codes="63" limehd:keyLabel="\?\n0⇣" limehd:keyEdgeFlags="right"/>
    </Row>

    <Row limehd:rowEdgeFlags="bottom">
        <Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done" limehd:iconPreview="@drawable/sym_keyboard_feedback_done" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="left"/>   
        <Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift" limehd:iconPreview="@drawable/sym_keyboard_feedback_shift" limehd:isModifier="true" 
                limehd:keyWidth="15%p" limehd:isSticky="true"/>             
        <Key limehd:codes="-9" 
            limehd:keyLabel="123\nEN"
			limehd:popupKeyboard="@xml/popup_symbol_mode"	  
            limehd:isModifier="true" limehd:keyWidth="10%p" />
        <Key limehd:codes="32" limehd:keyIcon="@drawable/sym_keyboard_space_lime" limehd:iconPreview="@drawable/sym_keyboard_feedback_space"  limehd:keyWidth="30%p" limehd:isRepeatable="true"/>
        <Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete" limehd:iconPreview="@drawable/sym_keyboard_feedback_delete" limehd:isModifier="true" 
                limehd:keyWidth="15%p" limehd:isRepeatable="true"/>
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return" limehd:iconPreview="@drawable/sym_keyboard_feedback_return" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"/>
    </Row>
    
    
</Keyboard>
    