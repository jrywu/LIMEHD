<Keyboard xmlns:limehd="http://schemas.android.com/apk/res/net.toload.main.hd"
    limehd:keyWidth="10%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key limehd:codes="126" limehd:keyLabel="~" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="177" limehd:keyLabel="±"/>
        <Key limehd:codes="215" limehd:keyLabel="×"/>
        <Key limehd:codes="247" limehd:keyLabel="÷"/>
        <Key limehd:codes="8226" limehd:keyLabel="•"/>
        <Key limehd:codes="176" limehd:keyLabel="°"/>
        <Key limehd:codes="96" limehd:keyLabel="`"/>
        <Key limehd:codes="180" limehd:keyLabel="´"/>
        <Key limehd:codes="123" limehd:keyLabel="{"/>
        <Key limehd:codes="125" limehd:keyLabel="}" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key limehd:codes="169" limehd:keyLabel="©" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="163" limehd:keyLabel="£"/>
        <Key limehd:codes="8364" limehd:keyLabel="€"/>
        <Key limehd:codes="94" limehd:keyLabel="^"/>
        <Key limehd:codes="174" limehd:keyLabel="®"/>
        <Key limehd:codes="165" limehd:keyLabel="¥"/>
        <Key limehd:codes="95" limehd:keyLabel="_"/>
        <Key limehd:codes="61" limehd:keyLabel="="/>
        <Key limehd:codes="91" limehd:keyLabel="["/>
        <Key limehd:codes="93" limehd:keyLabel="]" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift"
                limehd:keyWidth="15%p" limehd:isModifier="true"
                limehd:isSticky="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="161" limehd:keyLabel="¡" />
        <Key limehd:codes="60" limehd:keyLabel="&lt;"/>
        <Key limehd:codes="62" limehd:keyLabel="&gt;"/>
        <Key limehd:codes="162" limehd:keyLabel="¢"/>
        <Key limehd:codes="124" limehd:keyLabel="|"/>
        <Key limehd:codes="92" limehd:keyLabel="\\" />
        <Key limehd:codes="191" limehd:keyLabel="¿"/>
        <Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete" limehd:iconPreview="@drawable/sym_keyboard_feedback_delete" limehd:isModifier="true"
                limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"
                limehd:isRepeatable="true"/>
    </Row>
    
    <Row limehd:rowEdgeFlags="bottom">
        <Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done" limehd:isModifier="true" 
                limehd:keyWidth="15%p" limehd:keyEdgeFlags="left" />
        <Key limehd:codes="-2" limehd:isModifier="true" limehd:keyLabel="EN" limehd:keyWidth="10%p" />
        <Key limehd:codes="32" limehd:keyIcon="@drawable/sym_keyboard_space"
        	limehd:iconPreview="@drawable/sym_keyboard_feedback_space"
        	limehd:keyWidth="30%p" 
                limehd:isRepeatable="true"/>
        <Key limehd:codes="8230" limehd:keyLabel="…" limehd:keyWidth="15%p" />
        <Key limehd:codes="-10" limehd:isModifier="true" limehd:keyLabel="中"  limehd:keyWidth="10%p" />
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return" limehd:iconPreview="@drawable/sym_keyboard_feedback_return" limehd:isModifier="true"
                limehd:keyWidth="20%p" limehd:keyEdgeFlags="right" />
    </Row>
</Keyboard>
